n":4}},{"source":"styles.css","name":null,"original":{"line":225,"column":18},"generated":{"line":225,"column":18}},{"source":"styles.css","name":null,"original":{"line":226,"column":0},"generated":{"line":226,"column":0}}],"sources":{"styles.css":"@font-face {\n    font-family: 'roboto-mono';\n    src: url('/static/fonts/roboto-mono.ttf') format('tff') /* Modern browsers */\n}\n\n/* Universal styling for padding, margin, and box sizing */\n* {\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n}\n\nbody {\n    background-color: #000017;\n    font-family: 'roboto-mono', sans-serif;\n}\n\n/* GAME CONTAINER */\n/* Center main content */\n#game-content {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n}\n\n#game-div {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    background-color: #000017;\n    height: 700px;\n    width: 1000px;\n    padding: 50px;\n}\n\n#auth-ui, #profile-ui {\n    display: flex;\n    flex-direction: column;\n    height: 100%;\n    width: 100%;\n    row-gap: 10px;\n}\n\n/* LOGIN AND REGISTER PAGE*/\n#account-msg {\n    font-size: 14px;\n    font-weight: 300;\n    color: #41dba0;\n    letter-spacing: 0.03em;\n}\n\n#register-link, #login-link {\n    display: inline-block;\n    color: #ffc34b;\n    text-decoration: underline;\n    cursor: pointer;\n}\n\n#login-form, #register-form {\n    display: flex;\n    flex-direction: column;\n    row-gap: 10px;\n}\n\n#login-div {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    row-gap: 10px;\n}\n\n/* TYPOGRAPHY FOR LOGIN AND REGISTER PAGE */\n\n/* 'Welcome to spacedash' heading */\nh1 {\n    font-size: 3em;\n    font-weight: 300;\n    color: white;\n    padding: 50px 0;\n}\n\n/* 'spacedash' span within 'Welcome to spacedash' heading */\n.spacedash {\n    color: #41dba0;\n}\n\n/* PROFILE PAGE */\n#profile-ui {\n    display: flex;\n    flex-direction: column;\n    row-gap: 50px;\n}\n\n#sprite-select {\n    display: flex;\n    column-gap: 20px;\n    height: 120px;\n}\n\n/* Hide the radio input */\n.character-radio {\n    display: none;\n}\n\n/* Circular container around each image */\n.character-label {\n    display: inline-flex;\n    height: 120px;\n    width: 120px;\n    border-radius: 15px;\n    background-color: #191f2c;\n    border: 2px solid transparent; /* Initially transparent */\n    padding: 5px 5px 20px;\n    cursor: pointer;\n    transition: border-color 0.3s ease, box-shadow 0.3s ease;\n}\n\n/* Default style for images */\n.character-image {\n    object-fit: contain;\n    image-rendering: pixelated;\n    transition: transform 0.3s ease;\n}\n\n/* Scale up the image on hover */\n.character-label:hover .character-image {\n    transform: scale(1.2);\n}\n\n/* Change border color of label when the associated radio input is checked */\n.character-radio:checked + .character-label {\n    border-color: #41dba0; /* Blue border for selected state */\n    box-shadow: 0 0 5px rgba(65, 178, 135, 0.5); /* Optional glow effect */\n}\n\n#action-menu {\n    display: flex;\n    flex-direction: column;\n    row-gap: 10px;\n}\n\n/* TYPOGRAPHY FOR PROFILE PAGE */\n#character-heading {\n    color: #41dba0;\n    font-size: 1.1em;\n    font-weight: 400;\n    letter-spacing: 0.04em;\n    padding: 20px 0;\n}\n\n/* INPUT FIELDS */\n#username, #password, #create-username, #create-password, #confirm-password {\n    font-size: 14px;\n    line-height: 1.5;\n    background-color: #191f2c;\n    border-radius: 7px;\n    border: none;\n    color: #c8cad9;\n    padding: 5px 10px;\n    height: 40px;\n    width: 250px;\n}\n\n#username::placeholder, #password::placeholder, #create-username::placeholder,\n#create-password::placeholder, #confirm-password::placeholder {\n    color: #5a698a;\n}\n\n/* BUTTONS */\n#login-button, #register-button, #play-button,\n#stats-button, #leaderboard-button, #logout-button {\n    font-size: 14px;\n    background-color: #41dba0;\n    border-radius: 7px;\n    border: none;\n    cursor: pointer;\n    height: 40px;\n    width: 250px;\n    transition: background-color 0.3s ease, transform 0.1s ease;\n}\n\n/* Hover effect */\n#login-button:hover, #register-button:hover, #play-button:hover {\n    background-color: #87ffcf; /* Darker green on hover */\n}\n\n#login-button:active, #register-button:active, #play-button:active {\n    background-color: #b2ffde; /* Darker green on hover */\n}\n\n#register-button, #stats-button, #leaderboard-button {\n    background-color: #ffc34b;\n}\n\n#register-button:hover, #stats-button:hover, #leaderboard-button:hover {\n    background-color: #ffd37d;\n}\n\n#register-button:active, #stats-button:active, #leaderboard-button:active {\n    background-color: #ffe9c3;\n}\n\n#logout-button {\n    background-color: #e13960;\n}\n\n#logout-button:hover {\n    background-color: #f65279;\n}\n\n#logout-button:active {\n    background-color: #ffbecc;\n}\n\n/* ERRORS */\n#error-div {\n    display: flex;\n    align-items: center;\n    height: 70px;\n}\n\n#error-msg {\n    font-size: 14px;\n    color: #e13960;\n}"},"lineCount":null}},"error":null,"hash":"13dfb1598fc4fafe12af4d65bb282358","cacheData":{"env":{}}}